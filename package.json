{
  "name": "todomvc",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "json-server": "^0.14.2",
    "lodash": "^4.17.11",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.3.1",
    "redux-saga": "^1.0.0",
    "require-uncached": "^2.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.5.2",
    "reselect": "^3.0.1",
    "todomvc-app-css": "^2.1.1"
  },
  "scripts": {
    "start": "concurrently 'npm:frontend' 'npm:api'",
